{% extends 'base.html.twig' %}

{% block body %}
	{% include 'partner/_banner.html.twig' %}
	<main class="container">
		<div class="row">
			<div class="text-center my-5 row-4 rounded-2">
				<h2>Les partenaires</h2>
			</div>
			<div class="partnerLogo">
				<div class="container">
					{% for partner in partners %}
						<img class="col-1 my-3 mx-3" src={% if partner.picture is empty %} {% else %} {{ partner.picture }} {% endif %}>
					<p>
					{% if partner.picture is empty %}{% else %} {{ partner.name }} {% endif %} <a href=""></a>
					</p>
					{% endfor %}
				</div>
			</div>
		</div>
	</main>
{% endblock %}
