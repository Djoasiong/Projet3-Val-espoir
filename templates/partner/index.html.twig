{% extends 'base.html.twig' %}

{% block body %}
	{% include 'partner/_banner.html.twig' %}
	<main class="container">
		<div class="text-center my-5 row-4 rounded-2">
			<h2>Les partenaires institutionnels</h2>
		</div>
		<div class="mb-5">
			<p>- Pole Emploi</p>
			<p>- Maison Du Département</p>
			<p>- Mission Locale</p>
			<p>- Relais Emploi</p>
			<p>- Centre de Formation</p>
		</div>
		<div class="text-center my-5 row-2">
			<h2>Les partenaires sociaux</h2>
		</div>
		<div class="mb-5">
			<p>- Centre Communal d'Action Sociale (CCAS)</p>
			<p>- Maison de l'Habitat</p>
			<p>- Structures d'hébergements</p>
			<p>- Structures liées à la santé (ANPAA, APLEAT, CPAM...)</p>
		</div>
		<div class="text-center my-5 row-3">
			<h2>Les partenaires financiers</h2>
		</div>
		<div class="partnerLogo">
			<div class="container">
				{% for partenaire in partenaires %}
					<img class="col-1 mx-3" src="{{ partenaire.picture }}" alt="{{ partenaire.name }}">
				{% endfor %}
			</div>
		</div>
	</main>
{% endblock %}
